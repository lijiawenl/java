package leetcode;

import leetcode.subTree.TreeNode;
// input a tree £¬then output its mirror tree
public class mirriorTree {
	class TreeNode{
		int val;
		TreeNode left;
		TreeNode right;
	}
	 
void f(TreeNode root){
	if(root ==  null){
		return ;
	}
	if(root.left ==null && root.right==null){
		return;
	}
	
    TreeNode temp =null;
	
    temp = root.left;
    root.left =root.right;
    root.right =temp;
	f(root.left);
	f(root.right);
	
	return ;
}	
	
	
}
